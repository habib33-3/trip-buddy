{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "prisma/schema.prisma"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "type-check": {
      "dependsOn": ["^type-check"]
    },
    "db:generate": {
      "cache": false,
      "inputs": ["prisma/schema.prisma"],
      "outputs": ["node_modules/.prisma/**"]
    },
    "db:push": {
      "cache": false,
      "inputs": ["prisma/schema.prisma"]
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "seed": {
      "dependsOn": ["db:push", "^seed", "^db:push"],
      "inputs": ["prisma/**", "scripts/seed.ts", ".env*"]
    }
  }
}
