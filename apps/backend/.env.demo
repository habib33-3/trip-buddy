# String values
APP_EMAIL=no-reply@example.com
APP_NAME=Express API Template
REDIS_KEY_PREFIX=app_prefix

# URL values
DATABASE_URL=postgresql://user:password@localhost:5432/mydatabase
REDIS_URL=redis://localhost:6379

# Number values
PORT=3000
RATE_LIMIT_WINDOW_MS=60000
# REDIS_EXPIRATION is in seconds
REDIS_EXPIRATION=3600
# Duration values (valid formats like '1h', '7d', etc.)
ACCESS_TOKEN_EXPIRATION=1h
REFRESH_TOKEN_EXPIRATION=7d

# Secret values (keep secure). Use random â‰¥32-byte strings.
# For dev you can generate with: openssl rand -base64 32
ACCESS_TOKEN_SECRET=CHANGE_ME_access_token_secret_32_bytes_min
REFRESH_TOKEN_SECRET=CHANGE_ME_refresh_token_secret_32_bytes_min
